<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.error_notification %>

  <%= f.input :content, placeholder: "What do you think of that ?" %>
  <%= f.input :rating, collection: 0..5, selected: "3" %>

  <%= f.button :submit %>
<% end %>
<br>
<%= link_to "Back", restaurant_path(@restaurant) %>
